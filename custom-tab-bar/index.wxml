<cover-view class="tab-bar">
  <cover-view wx:for="{{isAdmin? adminList: userList}}" wx:key="index" class="tab-bar-item" data-path="{{item.pagePath}}" data-index="{{index}}" bindtap="switchTab">
    <cover-image class="tab-bar-item-cover-image" src="{{selectedTabIndex === index ? item.selectedIconPath : item.iconPath}}"></cover-image>
  </cover-view>
</cover-view>

